{
  "name": "web-starter",
  "description": "Web Starter Kit",
  "version": "0.0.0",
  "main": "src/index.html",
  "scripts": {
    "build": "yarn clean && parcel build src/index.html --public-url ./ --no-source-maps --detailed-report --out-dir=./dist",
    "build-production": "yarn clean && parcel build src/index.html --no-source-maps --detailed-report --out-dir=./dist",
    "clean": "shx rm -rf ./dist .cache",
    "dev:parcel": "parcel serve src/index.html",
    "release": "yarn build && now",
    "start": "run-p dev:*"
  },
  "dependencies": {
    "isomorphic-unfetch": "^3.0.0",
    "load-asset": "^1.2.0",
    "particlesjs": "^2.2.3",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-content-loader": "^4.2.1",
    "react-dom": "^16.8.6",
    "react-ga": "^2.6.0",
    "react-waypoint": "^9.0.2",
    "rellax": "^1.10.0",
    "styled-components": "^4.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/node": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "autoprefixer": "^9.6.0",
    "eslint": "^5.16.0",
    "eslint-plugin-react": "^7.13.0",
    "husky": "^2.4.1",
    "lint-staged": "^8.2.1",
    "npm-run-all": "^4.1.5",
    "parcel-bundler": "^1.11.0",
    "parcel-plugin-inlinesvg": "^0.1.1",
    "postcss-modules": "^1.4.1",
    "prettier": "^1.18.2",
    "pretty-quick": "^1.11.0",
    "sass": "^1.21.0",
    "shx": "^0.3.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && pretty-quick --staged"
    }
  },
  "license": "unlicense",
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "private": true
}
